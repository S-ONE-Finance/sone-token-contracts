    "build",
    "contracts"
  ],
  "license": "MIT",
  "repository": "https://github.com/S-ONE-Finance/soneswap-core.git",
  "keywords": [
    "soneswap",
    "ethereum",
    "uniswap",
    "sushiswap"
  ],
  "dependencies": {
    "@openzeppelin/contracts": "^4.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.0.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@truffle/hdwallet-provider": "^1.2.1",
    "dotenv": "^8.2.0",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "truffle": "^5.1.44",
    "truffle-flattener": "^1.4.4",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "scripts": {
    "precompile": "rimraf build .openzeppelin",
    "compile": "yarn truffle compile",
    "flatten": "mkdir -p build/flatten-contracts && yarn truffle-flattener",
    "test": "yarn truffle test",
    "deploy:ethereum":    "yarn truffle migrate --to 2 --network ethereum",
    "deploy:local":       "yarn truffle migrate --to 2 --network dev",
    "deploy:ropsten":     "yarn truffle migrate --to 2 --network ropsten",
    "deploy:rinkeby":     "yarn truffle migrate --to 2 --network rinkeby",
    "deploy:bsc":         "yarn truffle migrate --to 2 --network bsc",
    "deploy:bsc_testnet": "yarn truffle migrate --to 2 --network bsc_testnet",
    "deploy-upgrade:ethereum":    "yarn truffle migrate --to 3 --network ethereum",
    "deploy-upgrade:local":       "yarn truffle migrate --to 3 --network dev",
    "deploy-upgrade:ropsten":     "yarn truffle migrate --to 3 --network ropsten",
    "deploy-upgrade:rinkeby":     "yarn truffle migrate --to 3 --network rinkeby",
    "deploy-upgrade:bsc":         "yarn truffle migrate --to 3 --network bsc",
    "deploy-upgrade:bsc_testnet": "yarn truffle migrate --to 3 --network bsc_testnet"
  },
  "devDependencies": {
    "@openzeppelin/truffle-upgrades": "^1.5.1"
  }
}
